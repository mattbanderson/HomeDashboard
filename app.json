{
  "expo": {
    "name": "HomeDashboard",
    "description": "An app for controlling IoT devices at home.",
    "slug": "HomeDashboard",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.mattbanderson.homedashboard"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mattbanderson.homedashboard"
    }
  }
}
