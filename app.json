{
  "expo": {
    "name": "Home Control",
    "description": "An app for controlling IoT devices at home.",
    "slug": "home-control",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.mattbanderson.homecontrol"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mattbanderson.homecontrol"
    }
  }
}
